  "scripts": {
    "css:build": "rm -rf src/styles && node-sass-chokidar src/scss -o src/styles",
    "css:watch": "npm run css:build && node-sass-chokidar src/scss -o src/styles --watch --recursive",
    "server": "npm run css:build && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
